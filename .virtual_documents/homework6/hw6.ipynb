def calculate_brightness(magnitude_values):
    brightnesses_list = []
    total_brightness = 0
    
    for i in magnitude_values:
        brightnesses_list.append(10**((-i)/(2.5)))
        
    for j in brightnesses_list:
        total_brightness += j
    
    return brightnesses_list, total_brightness

